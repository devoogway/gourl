{
    "name": "devoogway/gourl",
    "description": "implementation of gourl.io payment gateway for Laravel framework",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "bitcoin",
        "crypto currency",
        "laravel",
        "payment gateway"
    ],
    "homepage": "https://www.github.com/devoogway/gourl",
    "extra": {
        "laravel": {
            "providers": [
                "Devoogway\\Gourl\\ServiceProvider"
            ],
            "aliases": {
                "Gourl": "Devoogway\\Gourl\\Facade"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Devoogway\\Gourl\\": "src/"
        }
    },
    "authors": [
        {
            "name": "DivineAlien",
            "email": "DivineAlien96@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "minimum-stability": "dev"
}
